@model IEnumerable<EQS.Models.WindowSet>

@{
    ViewBag.Title = "Window-Index";
}

<h2>Window</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UnavailableOperation)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CurrentClient)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RestOfTime)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UnavailableOperation)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CurrentClient)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RestOfTime)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
